<script>


    function handleSortChange(selectedOption) {
        const category = document.getElementById('categoryFilter').value || null;
        const brand = document.getElementById('brandFilter').value || null;
        const urlParams = new URLSearchParams(window.location.search);

        if (category) {
            urlParams.set('category', category);
        } else {
            urlParams.delete('category');
        }

        if (brand) {
            urlParams.set('brand', brand);
        } else {
            urlParams.delete('brand');
        }

        urlParams.set('sort', selectedOption);

        const newUrl = `${window.location.origin}/filter?${urlParams.toString()}`;
        window.location.href = newUrl;
    }

    function handleCategoryChange(selectedCategory) {
        const sort = document.getElementById('sortByDropdown').value;
        const brand = document.getElementById('brandFilter').value || null;
        const urlParams = new URLSearchParams(window.location.search);

        if (selectedCategory) {
            urlParams.set('category', selectedCategory);
        } else {
            urlParams.delete('category');
        }

        if (brand) {
            urlParams.set('brand', brand);
        } else {
            urlParams.delete('brand');
        }

        urlParams.set('sort', sort);

        const newUrl = `${window.location.origin}/filter?${urlParams.toString()}`;
        window.location.href = newUrl;
    }

    function handleBrandChange(selectedBrand) {
        const sort = document.getElementById('sortByDropdown').value;
        const category = document.getElementById('categoryFilter').value || null;
        const urlParams = new URLSearchParams(window.location.search);

        if (selectedBrand) {
            urlParams.set('brand', selectedBrand);
        } else {
            urlParams.delete('brand');
        }

        if (category) {
            urlParams.set('category', category);
        } else {
            urlParams.delete('category');
        }

        urlParams.set('sort', sort);

        const newUrl = `${window.location.origin}/filter?${urlParams.toString()}`;
        window.location.href = newUrl;
    }
</script>


